{
  "name": "@usf-org/zod",
  "type": "module",
  "version": "0.0.1",
  "description": "Zod schema for USF.",
  "author": "Naily Zero <zero@naily.cc> (https://naily.cc)",
  "keywords": [
    "zod",
    "schema",
    "usf"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsup -w",
    "prepublishOnly": "tsup"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  }
}
